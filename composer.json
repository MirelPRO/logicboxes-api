{
    "name": "netinternet/logicboxes",
    "description": "Laravel logicboxes package",
    "keywords": [
        "laravel",
        "logicboxes",
        "domain",
        "registrar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hakan ERSU",
            "email": "hakanersu@gmail.com"
        },
        {
            "name": "Baris ESEN",
            "email": "barisesennn@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.0",
        "illuminate/console": ">=5.0",
        "illuminate/filesystem": ">=5.0",
        "symfony/class-loader": "^2.3|^3.0"
    },
    "require-dev": {
        "illuminate/config": ">=5.0",
        "illuminate/view": ">=5.0",
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "^6.4",
        "guzzlehttp/guzzle": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Netinternet\\Logicboxes\\": "src",
            "Netinternet\\Logicboxes\\Test\\": "tests/"
        },
        "files" : [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Netinternet\\Logicboxes\\LogicboxesServiceProvider"
            ],
            "aliases": {
                "Logicboxes": "Netinternet\\Logicboxes\\Facades\\Logicboxes"
            }
        }
    }
}
